{
  "compilerOptions": {
    "strict": true,
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "types": ["vite/client"],
    "baseUrl": ".",
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true
  },
  "include": ["./"]
}
